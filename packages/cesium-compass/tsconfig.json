{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["es2020", "dom"],
    "isolatedModules": true,
    "noImplicitAny": true,
    "experimentalDecorators": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "noImplicitOverride": true
  }
}
